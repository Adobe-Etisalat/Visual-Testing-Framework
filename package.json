  },
  "homepage": "https://github.com/anithaparuchuri/Visual-Testing-Framework.git#readme",
  "dependencies": {
    "@percy/cli-command": "^1.10.4",
    "@types/cypress": "^1.1.3",
    "cypress-real-events": "^1.6.0",
    "mocha": "^10.0.0",
    "percy": "^5.0.0",
    "scroll-to-bottomjs": "^1.1.0"
  },
  "devDependencies": {
    "@percy/cli": "^1.10.4",
